{
    "manifest_version": 2,

    "name": "Supreme+ Bot",
    "version": "1.1.6",
    "description": "Advanced http://www.supremenewyork.com/ onlineshop automation",

    "options_page": "options/options.html",

    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "webNavigation",
        "*://www.supremenewyork.com/*"
    ],

    "icons":{
        "128": "pictures/icon.png"
    },

    "browser_action": {
        "default_icon": "pictures/icon.png",
        "default_popup": "popup/popup.html"
    },

    "background": {
        "scripts": ["initialisation.js", "myStorageHandler.js", "background.js"]
    }

}